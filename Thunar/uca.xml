<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open Terminal Here</name>
	<unique-id>1669028372810131-1</unique-id>
	<command>exo-open --working-directory %f --launch TerminalEmulator</command>
	<description>Example for a custom action</description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>cm_extractfiles</icon>
	<name>Extract here</name>
	<unique-id>1672936934089067-1</unique-id>
	<command>tar xf %n &amp;&amp; notify-send --urgency=critical --icon=cm_extractfiles &quot;Extraction is done&quot;</command>
	<description></description>
	<patterns>*.tar.bz2;*.tbz2;*.tar.gz;*.tar.xz</patterns>
	<other-files/>
</action>
<action>
	<icon>cm_extractfiles</icon>
	<name>Extract here</name>
	<unique-id>1672936974306158-2</unique-id>
	<command>unzip %f &amp;&amp; notify-send --urgency=critical --icon=cm_extractfiles &quot;Extraction is done&quot;</command>
	<description></description>
	<patterns>*.zip</patterns>
	<other-files/>
</action>
<action>
	<icon>application-x-compress</icon>
	<name>Create tar.gz</name>
	<unique-id>1672937977060878-3</unique-id>
	<command>tar czvf %n.tar.gz %n &amp;&amp; notify-send --urgency=critical --icon=file-roller &quot;Archive created&quot;</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
</action>
</actions>
